local tpMode = "Tween" -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô

-- === ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ï‡∏≤‡∏°‡πÇ‡∏´‡∏°‡∏î ===
local function TweenTP(cf)
    local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
    local dist = (hrp.Position - cf.Position).Magnitude
    game:GetService("TweenService"):Create(hrp, TweenInfo.new(dist / 300, Enum.EasingStyle.Linear), {CFrame = cf}):Play()
end

local function InstantTP(cf)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cf
end

local function BypassResetTP(cf)
    local player = game.Players.LocalPlayer
    local char = player.Character or player.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")

    -- ‡∏ö‡∏¥‡∏ô‡πÄ‡∏£‡πá‡∏ß‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡πÄ‡∏Å‡∏≤‡∏∞
    local speed = 3000
    local dist = (hrp.Position - cf.Position).Magnitude
    local duration = dist / speed

    game:GetService("TweenService"):Create(hrp, TweenInfo.new(duration, Enum.EasingStyle.Linear), {CFrame = cf + Vector3.new(0, 5, 0)}):Play()
    wait(duration + 0.1)

    -- ‡∏ö‡∏≤‡∏¢‡∏û‡∏≤‡∏™ 3 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á
    for i = 1, 3 do
        if char and char:FindFirstChild("HumanoidRootPart") then
            char.HumanoidRootPart.CFrame = cf + Vector3.new(0, 3, 0)
            wait(0.15)
        end
    end

    -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï
    wait(0.2)
    player.Character:BreakJoints()
end

local function TPTo(cf)
    if tpMode == "Tween" then
        TweenTP(cf)
    elseif tpMode == "Instant" then
        InstantTP(cf)
    elseif tpMode == "Bypass + Reset" then
        BypassResetTP(cf)
    end
end

-- === ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ã‡∏µ 1 ‚Üí ‡∏ã‡∏µ 2 ‚Üí ‡∏ã‡∏µ 3 ===
for _, sea in ipairs({"‡∏ã‡∏µ 1", "‡∏ã‡∏µ 2", "‡∏ã‡∏µ 3"}) do
    local tab = ui:MakeTab({sea})
    for island, cf in pairs(IslandCFrame[sea]) do
        tab:AddButton({
            Name = island,
            Callback = function()
                TPTo(cf)
            end
        })
    end
end

-- === ‡πÅ‡∏ó‡πá‡∏ö‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ ===
local settings = ui:MakeTab({"‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤"})
settings:AddDropdown({
    Name = "‡πÇ‡∏´‡∏°‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà",
    Default = tpMode,
    Options = {"Tween", "Instant", "Bypass + Reset"},
    Callback = function(value)
        tpMode = value
    end
})

settings:AddParagraph({
    Title = "‡πÄ‡∏•‡∏∑‡∏≠‡∏Å Tween ‡∏´‡∏≤‡∏Å‡∏Å‡∏•‡∏±‡∏ß‡∏ñ‡∏π‡∏Å‡πÅ‡∏ö‡∏ô, Bypass + Reset ‡∏Å‡πá‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢‡πÅ‡∏ï‡πà‡∏ä‡πâ‡∏≤‡∏Å‡∏ß‡πà‡∏≤ Tween",
    Desc = "Tween: ‡∏ö‡∏¥‡∏ô‡∏•‡∏∑‡πà‡∏ô‡πÑ‡∏´‡∏•\nInstant: ‡∏Å‡∏£‡∏∞‡πÇ‡∏î‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ\nBypass + Reset: ‡∏ö‡∏¥‡∏ô‡πÄ‡∏£‡πá‡∏ß ‚Üí ‡∏ö‡∏≤‡∏¢‡∏û‡∏≤‡∏™ 3 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á ‚Üí ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï"
})
